{
  "name": "quark-router",
  "version": "1.0.0",
  "description": "A router for Quark",
  "type": "module",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./router": {
      "import": "./lib/router.js",
      "types": "./lib/router.d.ts"
    },
    "./routes": {
      "import": "./lib/routes.js",
      "types": "./lib/routes.d.ts"
    },
    "./quark-link": {
      "import": "./lib/quark-link.js",
      "types": "./lib/quark-link.d.ts"
    }
  },
  "browserslist": "chrome >= 53, edge >= 79, firefox >= 63, opera >= 40, safari >= 10.1, ChromeAndroid >= 53,  FirefoxAndroid >= 63, ios_saf >= 10.3, Samsung >= 6.0",
  "files": [
    "lib/*",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "dev": "rollup -c --watch",
    "build": "vite build --config vite.config.build.ts && vite build --config vite.config.build-umd.ts && tsc --emitDeclarationOnly",
    "changelog": "conventional-changelog -p angular -n './changelog-config.cjs' -i CHANGELOG.md -s"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.8",
    "tslib": "^2.6.2",
    "urlpattern-polyfill": "^9.0.0"
  },
  "peerDependencies": {
    "quarkc": "^1.0.41"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-proposal-decorators": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@types/node": "^14.18.52",
    "typescript": "^5.4.2",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^3.8.1"
  },
  "keywords": [
    "router",
    "quarkc",
    "web-components"
  ],
  "author": "nieyt",
  "license": "ISC"
}
