{
  "name": "quark-router",
  "version": "1.0.0",
  "description": "A router for Quark",
  "type": "module",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./router": {
      "import": "./lib/router.js",
      "types": "./lib/router.d.ts"
    },
    "./routes": {
      "import": "./lib/routes.js",
      "types": "./lib/routes.d.ts"
    },
    "./quark-link": {
      "import": "./lib/quark-link.js",
      "types": "./lib/quark-link.d.ts"
    }
  },
  "browserslist": "chrome >= 53, edge >= 79, firefox >= 63, opera >= 40, safari >= 10.1, ChromeAndroid >= 53,  FirefoxAndroid >= 63, ios_saf >= 10.3, Samsung >= 6.0",
  "files": [
    "lib/*",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "dev": "rollup -c --watch",
    "build": "rimraf lib && rollup -c"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.8",
    "tslib": "^2.6.2",
    "urlpattern-polyfill": "^9.0.0"
  },
  "peerDependencies": {
    "quarkc": "^1.0.41"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-proposal-decorators": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/node": "^14.18.52",
    "rimraf": "3.0.2",
    "rollup": "2.77.0",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "router",
    "quarkc",
    "web-components"
  ],
  "author": "nieyt",
  "license": "ISC"
}
